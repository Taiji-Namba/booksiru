<div class = "content-wrapper"> 
  <h1>お気に入り</h1>
  <br>
  <h2>シリーズ一覧</h2>
  <div class = "container-fluid" >
    <div class = "row">
      <%= render partial: "favorite_series", collection: @favored_books, as: "book" %>
    </div>
  </div>

  <h2>作家一覧</h2>
  <div class = "container-fluid" >
    <div class = "row">
      <%= render partial: "favorite_author", collection: @favored_authors, as: "book" %>
    </div>
  </div>
</div>